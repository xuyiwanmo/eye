<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:fitsSystemWindows="true"
    tools:context=".consult.openConsult.activity.OpenViedoActivity">

    <com.sg.eyedoctor.common.view.MyActionbar
        app:rightBtn_visible="false"
        app:actionbar_title="@string/video_consult"
        android:id="@+id/actionbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"/>

    <ImageView
        android:id="@+id/set_img"
        android:layout_marginBottom="24dp"
        android:src="@drawable/home_phone_consult"
        android:layout_width="match_parent"
        android:layout_height="144dp"/>

    <TextView
        android:id="@+id/detail_tv"
        android:textColor="@color/text_body_strong"
        android:textSize="@dimen/text_16"
        android:lineSpacingExtra="7dp"
        android:layout_gravity="center_horizontal"
        android:text="@string/not_open_consult"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        />

    <TextView
        android:id="@+id/open_tv"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/bg_blue_text_white"
        android:gravity="center"
        android:text="@string/open_now"
        android:textColor="@color/white"
        android:textSize="18sp" />

</LinearLayout>
