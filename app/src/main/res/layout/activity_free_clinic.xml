<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context="com.sg.eyedoctor.helpUtils.freeClinic.activity.FreeClinicActivity"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.sg.eyedoctor.common.view.MyActionbar
        android:id="@+id/actionbar"
        app:rightBtn_visible="false"
        app:actionbar_title="@string/free_clinic_publish"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <com.pulltorefresh.chuwe1.swipemenu.library.swipemenuview.SwipeMenuListView
        android:divider="@color/trans"
        android:id="@+id/free_clinic_lv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

</LinearLayout>
